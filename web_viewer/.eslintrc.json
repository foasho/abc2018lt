{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },  
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn",
        "no-extra-semi": "warn",
        "no-undef": "warn"
    }
}